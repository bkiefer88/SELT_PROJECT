!!! 5
%html
  %head
    %title Technology Education!
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags

  %head
    %h1.title Technology Education!
    #main
      - if flash[:notice] && flash[:level_complete]
        #notice.level_complete= flash[:level_complete]
        #notice.message= flash[:notice]
      - elsif flash[:notice]
        #notice.message= flash[:notice]
        %br
      - elsif flash[:warning]
        #warning.message= flash[:warning]
        %br
      - elsif flash[:error]
        #error.message= flash[:error]
        %br
      - else
        %br
        %br

      - if signed_in?
        #applink= link_to "Sign-Out", signout_path, :method => :delete, :id => "Sign-Out"
        #applink= link_to "Profile", user_path(current_user), :method => :get, :id => "Profile"
        #applink= link_to "Home", home_path, :method => :get
        #userinfo
          Current User: 
          = current_user.username
      - else
        #applink= link_to "Sign-In", signin_path, :method => :get, :id => "Sign-In"
        #applink= link_to "Sign-Up", signup_path, :method => :get, :id => "Sign-Up"
      
      %br
      %br
      = yield
      = render :partial => 'layouts/footer'
